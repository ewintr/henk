# Example configuration file for henk
# Copy this file to .henk/config.toml and modify as needed

# Multiple providers can be configured
# If no model is marked as default, the first model from the first provider will be used
[[providers]]
type = "claude"
# Optional: API key (if required by the service)
# api_key = "your-api-key-here"

  [[providers.models]]
  name = "claude-3-5-sonnet"

  [[providers.models]]
  name = "claude-3-haiku"

[[providers]]
type = "openai"
base_url = "http://192.168.178.12:11434/v1"
# Optional: API key (if required by the service)
# api_key = "your-api-key-here"

  [[providers.models]]
  name = "llama3.1:8b"
  default = true

  [[providers.models]]
  name = "mistral:7b"

[[providers]]
type = "ollama"
base_url = "http://localhost:11434"

  [[providers.models]]
  name = "llama3.1:8b"

  [[providers.models]]
  name = "mistral:7b"
